---
layout: none
permalink: /blog/feed.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Blog de Fonasa.info</title>
  <subtitle>Art√≠culos sobre salud, SNIS, Fonasa y prestaciones en Uruguay.</subtitle>
  <link href="{{ site.url }}/blog/feed.xml" rel="self"/>
  <link href="{{ site.url }}/blog/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/blog/</id>
  <generator>Jekyll</generator>

  {% assign blog_posts = site.posts %}
  {% for post in blog_posts %}
    <entry>
      <title>{{ post.title | escape }}</title>
      <link href="{{ site.url }}{{ post.url }}"/>
      <id>{{ site.url }}{{ post.url }}</id>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <summary>{{ post.description | strip_html | truncate: 160 }}</summary>
    </entry>
  {% endfor %}
</feed>